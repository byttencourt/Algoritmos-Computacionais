algoritmo "exercicio antecessor do numero e sucessor"
// Função :
// Autor :
// Data : 10/03/2022
// Seção de Declarações 
var
   num, dobro, terca: Real

inicio
escreva ("Informe um Numero: ")
leia (num)
dobro <- (num * 2)
terca <- num / 3)
escreval ("O dobro de ", num, " é", dobro,".")
escreva ("A terça parte de ", num, " é ", terca:5:2,".")
// Seção de Comandos 
fimalgoritmo

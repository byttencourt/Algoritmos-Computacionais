algoritmo "exercicio antecessor do numero e sucessor"
// Função :
// Autor :
// Data : 10/03/2022
// Seção de Declarações 
var
   num, ant, suc: Inteiro

inicio
escreva ("Informe o Numero: ")
leia (num)
ant <- (num - 1)
suc <- (num + 1)
escreva ("O antecessor de ", num, " é", ant," e o sucessor é", suc)
// Seção de Comandos 
fimalgoritmo
